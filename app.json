{
  "name": "PM Chat Bot",
  "description": "A Simple Feed-Back Bot Written In PHP, Which Is Similar To The Livegram Bot.",
  "keywords": [
    "telegram",
    "bot",
    "php"
  ],
  "repository": "https://github.com/vectorx-dev/PMChatBot",
  "logo": "https://telegra.ph/file/46b333ff3368591dd4b4d.jpg",
  "env": {
    "BOT_TOKEN": {
      "description": "Enter Your Bot Token Id, Generated From @Botfather",
