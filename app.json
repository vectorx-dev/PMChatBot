    "DB_TUR": {
      "description": "db or json",
      "value": "json",
      "required": false
    },
    "OWNER_ID": {
      "description": "Enter Your Telegram Id.",
      "required": true
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
